{
    "name": "alive2212/laravel-excel-helper",
    "type": "library",
    "description": "this is excel helper package for download table in pdf or excel format",
    "keywords": [
        "Alive2212",
        "ExcelHelper",
        "PDF",
        "laravel"
    ],
    "homepage": "https://github.com/Alive2212/ExcelHelper",
    "license": "MIT",
    "authors": [
        {
            "name": "babak nodoust",
            "email": "alive2212@yahoo.com",
            "homepage": "https://www.linkedin.com/in/babak-nodoust-a7489543/",
            "role": "CTO"
        }
    ],
    "require": {
        "illuminate/support": "~7",
        "maatwebsite/excel": "*",
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "laravel/framework": "~7",
        "maatwebsite/excel": "~2.1.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Alive2212\\ExcelHelperApp\\": "",
            "Alive2212\\ExcelHelperTest\\": "tests",
            "Alive2212\\ExcelHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alive2212\\ExcelHelperApp\\": "",
            "Alive2212\\ExcelHelperTest\\": "tests",
            "Alive2212\\ExcelHelper\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
